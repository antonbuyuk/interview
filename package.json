{
  "name": "frontend-interview-app",
  "version": "1.0.0",
  "type": "module",
  "engines": {
    "node": ">=22.12.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "vite",
    "dev:server": "npx tsx watch server/index.ts",
    "dev:client": "vite",
    "dev:all": "concurrently --kill-others-on-fail --handle-input --raw \"npm run dev:server\" \"npm run dev:client\"",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "deploy": "npm run build && echo 'Build completed. Push to main branch to deploy via GitHub Actions.'",
    "start": "npx tsx server/index.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio",
    "migrate:data": "node scripts/migrate-to-db.js",
    "translate:questions": "node scripts/translate-questions.js",
    "postinstall": "prisma generate",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "fix": "prettier --write . && eslint . --fix && vue-tsc --noEmit"
  },
  "dependencies": {
    "@heroicons/vue": "^2.2.0",
    "@prisma/client": "^5.19.1",
    "@tiptap/extension-code-block": "^3.15.1",
    "@tiptap/starter-kit": "^3.15.1",
    "@tiptap/vue-3": "^3.15.1",
    "@vitalets/google-translate-api": "^9.2.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "groq-sdk": "^0.11.0",
    "highlight.js": "^11.10.0",
    "marked": "^11.2.0",
    "pinia": "^3.0.4",
    "tsx": "^4.21.0",
    "turndown": "^7.2.2",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.10",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/node": "^22.10.2",
    "@types/turndown": "^5.0.6",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-prettier": "^9.0.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.23.0",
    "prettier": "^3.2.5",
    "prisma": "^5.19.1",
    "sass": "^1.97.2",
    "typescript": "^5.7.2",
    "vite": "^7.3.0",
    "vue-tsc": "^2.1.10"
  }
}
